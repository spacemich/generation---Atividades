package AtividadePraticaJava8;

import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {

		int idade, count=-1;
		
		Scanner leia = new Scanner (System.in);
		
		System.out.println("Digite sua idade");
		idade = leia.nextInt();
				
		while (count <-1) {
			System.out.println(idade);
			count++;
	}

}
}
