package operadores;

public class aritimeticos {

	public static void main(String[] args) {
		int x = 4;
		int y = 2;
		int a = 3;
		int resul;
		
		resul = x *(y + a);
		
		x += y;
		
		//resul = x +y;
		
		System.out.println ("O Resultado é: " + resul);
		System.out.println ("O Resultado é: " + (y+a));
		System.out.println ("O Resultado é: " + x);
	}

}
